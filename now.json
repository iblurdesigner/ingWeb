{
    "version": 2,
    "name": "turismo-loja-app3",
    "builds": [
        {"use": "@now/static-build", "src": "package.json"}
    ],
    "routes": [
        {
            "src": "(.*).js", "dest": "/$1.js"
        },
        {
            "src": "(.*).png", "dest": "/$1.png"
        },
        {
            "src": "(.*).jpg", "dest": "/$1.jpg"
        },
        {
            "src": "(.*).css", "dest": "/$1.css"
        },
        {
            "src": "(.*).json", "dest": "/$1.json"
        },
        {
            "src": "/.*", "dest": "/index.html"
        }
    ]
}